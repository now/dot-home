INSTALL = ginstall
ZSHELL = /opt/local/bin/zsh

audacityuserconfdir = ~/Library/Application\ Support/audacity
firefoxuserconfdir = $(call shell_quote,$(wildcard ~/Library/Application\ Support/Firefox/Profiles/*.default))
guiuserconfdir = ~/Library/Preferences
vlcuserconfdir = $(guiuserconfdir)/org.videolan.vlc
