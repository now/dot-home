#! @SHELL@

if test $# -eq 0; then
  printf '%s: missing file argument\n' "expr "//$0" : '.*/\(.*\)'" >&2
  exit 1
fi

status=0

for f in "$@"; do
  test -f "$f" ||
    { printf '%s: canâ€™t unpack file (no such file)\n' "$f" >&2
      status=1
      continue; }
  case $f in -*) f=./$f ;; esac
  case "$f" in
    *.tar) tar xf "$f" ;;
    *.tar.bz2|*.tbz2) bunzip2 "$f" | tar x ;;
    *.tar.gz|*.tgz) gunzip "$f" | tar x ;;
    *.tar.lzma) unlzma "$f" | tar x ;;
    *.tar.xz) unxz "$f" | tar x ;;
    *.tar.Z) uncompress "$f" | tar x ;;
    *.gz) gunzip "$f" ;;
    *.bz2) bunzip2 "$f" ;;
    *.rar) unrar x -c- -o+ -idq "$f" ;;
    *.zip|*.jar|*.oxt) unzip -q -o "$f" ;;
    *.Z) uncompress -f "$f" ;;
    *.7z) 7z x "$f" ;;
    *) printf '%s: unknown format (unrecognized extension)\n' "$f" >&2; false ;;
  esac || status=1
done

exit $status
