path[1,0]=(. ~/bin)

LANG=en_US.UTF-8
LC_COLLATE=C

PAGER=vimless
MANPAGER="col -b | vimless +'setf man' -"
EDITOR=vim

MANWIDTH=79
SYSSCREENRC=/dev/null

declare -U path manpath

export                                                                \
  MANPATH                                                             \
  LANG LC_COLLATE                                                     \
  PAGER MANPAGER EDITOR                                               \
  MANWIDTH SYSSCREENRC

source $ZDOTDIR/os/profile
