_paste-cygwin-clipboard-path () {
  LBUFFER+=${(q)"$(cygpath "$(clipboard 2> /dev/null)" 2> /dev/null)"}
}
zle -N paste-cygwin-clipboard-path _paste-cygwin-clipboard-path
bindkey "^O^Y" paste-cygwin-clipboard-path
bindkey "^Oy" paste-cygwin-clipboard-path

alias kill='command kill -f'
alias push-ruby='rsync -rltgopD --modify-window=1 /Ruby19/*~*/devkit //Filer/Programs/Ruby19'
alias push-norway='rsync -rltpD --modify-window=1 /Ruby19/*~*/devkit //10.100.3.24/ames-ruby$/Ruby19'
